# Example environment configuration for Team Evil Jenkins
# Copy this file to `.env` and fill in real values before running build-all.sh

# Open WebUI (LLM) configuration
OPENWEBUI_URL=https://gpu.example.com
OPENWEBUI_API_KEY=your-openwebui-api-key

# Auth0 configuration
AUTH0_DOMAIN=your-tenant.eu.auth0.com
AUTH0_AUDIENCE=https://your-tenant.eu.auth0.com/api/v2/
AUTH0_CLIENT_ID=your-auth0-client-id
AUTH0_CLIENT_SECRET=your-auth0-client-secret
